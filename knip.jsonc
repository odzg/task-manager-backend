{
  "$schema": "https://unpkg.com/knip@latest/schema-jsonc.json",
  "entry": [
    "src/routes/**/*.ts", // Route files detected by `@fastify/autoload`
  ],
  "ignoreDependencies": [
    "@prisma/client", // Referenced in auto-generated Prisma client files, which are gitignored.
  ],
  "treatConfigHintsAsErrors": true,
}
