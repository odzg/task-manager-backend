{
  "$schema": "https://unpkg.com/knip@latest/schema-jsonc.json",
  "entry":
    // Route files detected by `@fastify/autoload`
    "src/routes/**/*.ts",
  "ignoreDependencies": [
    // Referenced in auto-generated Prisma client files, which are gitignored.
    "@prisma/client",
  ],
  "prisma": {
    "entry": "prisma/seed.ts",
  },
  "treatConfigHintsAsErrors": true,
}
