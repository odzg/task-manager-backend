{
  "$schema": "https://unpkg.com/knip@5/schema-jsonc.json",
  "entry":
    // Route files detected by `@fastify/autoload`
    "src/routes/**/*.ts",
  "ignoreDependencies": [
    // Referenced in auto-generated Prisma client files, which are gitignored.
    "@prisma/client",
  ],
  "node": {
    "entry":
      // Test files detected by Node test runner
      "**/*.test.ts",
  },
  "treatConfigHintsAsErrors": true,
}
